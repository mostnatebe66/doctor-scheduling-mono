    "build": "npm run build:api && npm run build:web",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "compose": "^0.1.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "docker": "^1.0.0",
    "mongo": "^0.1.0",
    "prettier": "^3.6.2",
    "up": "^1.0.2",
    "wait-on": "^9.0.1"
  },
  "dependencies": {
    "mongoose": "^8.19.1"
  }
