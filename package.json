{
  "name": "pds-schedule-app",
  "version": "1.0.0",
  "description": "A simple full-stack app for managing a doctor's daily schedule.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "cross-env MONGO_URI=mongodb://127.0.0.1:27017/pds_schedule npm --prefix doctor-scheduling-api run seed",
